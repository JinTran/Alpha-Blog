<% @article.comments.each do |comment| %>
  <p>
    <strong>Commenter:</strong>
    <%= comment.commenter %>
  </p>
  <p>
    <strong>Body:</strong>
    <%= comment.body %>
  </p>
  <p>
    <%= link_to "Delete" , [comment.article, comment],
                          method: :delete,
                          data: {confirm: "Are u sure?"}%>
  </p>

<% end %>